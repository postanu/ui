.h1
	font-size: var(--p-h1-font-size)
	font-weight: var(--p-h1-font-weight)
	line-height: var(--p-h1-line-height)

.h2
	font-size: var(--p-h2-regular-font-size)
	font-weight: var(--p-h2-regular-font-weight)

.headline
	color: var(--color-white-03)
	font-size: var(--p-headline-font-size)
	font-weight: var(--p-headline-font-weight)
	line-height: var(--p-headline-line-height)
	text-transform: uppercase
